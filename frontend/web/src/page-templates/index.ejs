<!DOCTYPE html>
<head>
  <meta charset="utf-8">
  <title><%= htmlWebpackPlugin.options.defaultTitle %></title>
  <% for (var filename in htmlWebpackPlugin.files.css) { %>
  <link href="<%= htmlWebpackPlugin.files.css[filename] %>" rel="stylesheet">
  <% } %>
</head>
    <body>
        <% if (htmlWebpackPlugin.options.appMountId) { %>
        <div id="<%= htmlWebpackPlugin.options.appMountId %>"></div>
        <% } %>

        <% for (var chunkname in htmlWebpackPlugin.files.chunks) { %>
        <script src="<%= htmlWebpackPlugin.files.chunks[chunkname].entry %>"></script>
        <% } %>
    </body>
</html>
